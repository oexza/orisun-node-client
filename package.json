{
  "name": "@orisun/eventstore-client",
  "version": "1.0.0",
  "description": "Node.js client for Orisun Event Store",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "example": "ts-node examples/basic-usage.ts",
    "generate-proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated --ts_out=grpc_js:./src/generated -I ../../eventstore ../../eventstore/eventstore.proto"
  },
  "keywords": [
    "eventstore",
    "grpc",
    "event-sourcing",
    "orisun"
  ],
  "author": "Orisun Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oexza/orisun.git",
    "directory": "clients/node"
  },
  "homepage": "https://github.com/oexza/orisun#readme",
  "bugs": {
    "url": "https://github.com/oexza/orisun/issues"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.6",
    "@grpc/proto-loader": "^0.7.10",
    "google-protobuf": "^3.21.2"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}